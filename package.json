{
  "name": "jfisher-code-challenge-excel",
  "version": "1.0.0",
  "description": "Josh Fisher's Version of the Whitebox Excel Code Challenge",
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "ttsc",
    "clean": "rm -rf coverage lib",
    "dev": "onchange 'src/**/*.ts' -- npm run start",
    "start": "npm run clean && npm run build && node lib/index.js",
    "test:clearCache": "jest --clearCache",
    "test": "jest --env=node --colors --coverage",
    "test:watch": "jest --env=node --colors --coverage --watchAll",
    "view:coverage": "serve coverage/lcov-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josh-fisher/code-challenge-excel.git"
  },
  "keywords": [
    "code-challenge",
    "excel",
    "whitebox",
    "jfisher",
    "code",
    "challenge"
  ],
  "author": "jfisher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/josh-fisher/code-challenge-excel/issues"
  },
  "homepage": "https://github.com/josh-fisher/code-challenge-excel#readme",
  "dependencies": {
    "@types/json2csv": "^5.0.1",
    "@types/lodash": "^4.14.168",
    "json2csv": "^5.0.6",
    "lodash": "^4.17.21",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2",
    "ts-transformer-keys": "^0.4.3",
    "typescript-is": "^0.17.1",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "onchange": "^7.1.0",
    "serve": "^11.3.2",
    "ts-jest": "^26.5.4",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.3"
  }
}
